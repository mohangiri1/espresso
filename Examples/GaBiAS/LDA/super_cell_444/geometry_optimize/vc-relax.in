&CONTROL
  calculation = 'vc-relax'
  prefix = 'GaBiAs'
  outdir      = './',
  pseudo_dir  = './',
  disk_io = 'none'
/

&SYSTEM
  degauss = 0.002
  ecutrho = 800
  ecutwfc = 80
  ibrav = 2
  celldm(1) = 42.405451184
